<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace ="database.ProductMapper">
  <insert id="registerProduct" parameterType = "Product">
  
  insert into PRODUCT_INFO values(num_product.nextval,#{category},#{p_name},#{rent_price},#{p_quality},#{p_content},"대여가능","대여가능",#{gender},#{image_file})
  </insert>
 
  </mapper>